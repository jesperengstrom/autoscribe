{
  "extends" : [
      "eslint:recommended",
      "airbnb",
      "prettier"
  ],
  "plugins": ["prettier"],
  "env": {
      "browser": true,
      "es6": true
  },
  "parser": "babel-eslint",
  "rules" : {
      "prettier/prettier": ["error", {"singleQuote": true, "trailingComma": "all"}],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "linebreak-style": 0
  }
}